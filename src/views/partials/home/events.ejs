<section class="upcoming-meetings" id="meetings">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="section-heading">
          <h2>Upcoming Events</h2>
        </div>
      </div>
      <div class="col-lg-12">
        <div class="row">
          <% upcomingEventsContent.forEach(event => { %>
          <div class="col-lg-4">
            <div class="meeting-item">
              <div class="thumb">
                <a href="/"><img src="<%= event.imageUrl %>" alt="<%= event.title %>"></a>
              </div>
              <div class="down-content">
                <div class="date">
                  <h6><%= event.month %> <span><%= event.day %></span></h6>
                </div>
                <a href="/">
                  <h4><%= event.title %></h4>
                </a>
                <p class="tbc_cap"><%= event.description %><br></p>
              </div>
            </div>
          </div>
          <% }); %>
        </div>
      </div>
    </div>
  </div>
</section>