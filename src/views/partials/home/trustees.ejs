<section class="our-courses" id="courses">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="section-heading">
          <h2>BOARD OF TRUSTEES</h2>
        </div>
      </div>
      <div class="col-lg-12">
        <div class="owl-courses-item owl-carousel">
          <% trusteeContent.forEach(member=> { %>
          <div class="item">
            <img src="<%= member.imageUrl %>" style="height: 250px; border-radius: 10px 10px 0px 0px;" alt="">
            <div class="down-content">
              <h4 style="height:100px; background-color: white;"><%- member.name %></h4>
              <div class="info">
                <div class="row">
                  <div>
                    <%= member.info %>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% }); %>

        </div>
      </div>
    </div>
  </div>
</section>