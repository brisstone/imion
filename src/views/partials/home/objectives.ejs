<section class="apply-now" id="apply">
    <div class="container">
        <div class="row">
            <% objectiveContent.forEach(function(item) { %>
                <div class="col-lg-6 align-self-center">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="item">
                                <h3>
                                    <%= item.title %>
                                </h3>
                                <p>
                                    <%= item.description %>
                                </p>
                                <div class="<%= item.buttonColor ? 'main-button-red' : 'main-button-yellow' %>">
                                    <div class="scroll-to-section">
                                        <a href="<%= item.buttonLink %>">
                                            <%= item.buttonLabel %>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <% }); %>
        </div>
    </div>
</section>